<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="WhiteColor" R="255" G="255" B="255"  A="255"/>
    <Color x:Key="LightGrayColor" R="0xe1" G="0xea" B="0xef"  A="255"/>
    <Color x:Key="MediumGrayColor" R="0x93" G="0x95" B="0x98"  A="255"/>
    <Color x:Key="DarkGrayColor" R="0x53" G="0x56" B="0x5a"  A="255"/>
    <Color x:Key="BlackColor" R="0" G="0" B="0"  A="255"/>
    <Color x:Key="PrimaryColor" R="0xBF" G="0x4D" B="0x28"  A="255"/>
    <Color x:Key="PrimaryAccent" R="0xE6" G="0xAC" B="0x27"  A="255"/>

    <SolidColorBrush x:Key="WhiteColorBrush" Color="{DynamicResource WhiteColor}"/>
    <SolidColorBrush x:Key="LightGrayColorBrush" Color="{DynamicResource LightGrayColor}"/>
    <SolidColorBrush x:Key="MediumGrayColorBrush" Color="{DynamicResource MediumGrayColor}"/>
    <SolidColorBrush x:Key="DarkGrayColorBrush" Color="{DynamicResource DarkGrayColor}"/>
    <SolidColorBrush x:Key="BlackColorBrush" Color="{DynamicResource BlackColor}"/>
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{DynamicResource PrimaryColor}"/>
    <SolidColorBrush x:Key="PrimaryAccentBrush" Color="{DynamicResource PrimaryAccent}"/>
    
    <!-- Fonts -->
    <FontFamily x:Key="PrimaryFont">">Segoe UI, Microsoft YaHei, Microsoft JhengHei, Meiryo UI</FontFamily>

</ResourceDictionary>